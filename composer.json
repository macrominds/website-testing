{
    "name": "macrominds/website-testing",
    "description": "Cross-Platform Helper for functional testing of websites.",
    "license": "MIT",
    "authors":[
        {
            "name": "Thomas Praxl",
            "email": "thomas@macrominds.de",
            "homepage": "http://www.macrominds.de"
        }
    ],
    "keywords": [
        "testing"
    ],
    "require": {
        "php": "^5.6 || ^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload":{
        "psr-4": {"macrominds\\website\\testing\\": "src/"}
    },
    "autoload-dev":{
        "psr-4": {"macrominds\\website\\testing\\": "tests/"}
    }
}
